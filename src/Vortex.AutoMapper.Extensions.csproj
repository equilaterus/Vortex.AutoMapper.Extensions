    <Authors>Equilaterus</Authors>
    <Company>Equilaterus</Company>
    <Description>Vortex extensions for automapper</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Equilaterus.Vortex.AutoMapper.Extensions</PackageId>
    <Version>0.1.0</Version>
    <Copyright>Daniel Cañizares and Juan Felipe Cañizares @ 2019</Copyright>
